<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lods"
    tools:context=".PathDecisionOutcome">

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/gifImageView5"
        android:layout_width="205dp"
        android:layout_height="230dp"
        android:layout_marginStart="188dp"
        android:layout_marginTop="488dp"
        android:src="@drawable/maoni"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/gifImageView"
        android:layout_width="60dp"
        android:layout_height="62dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="468dp"
        android:src="@drawable/hiok"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/gifImageView4"
        android:layout_width="70dp"
        android:layout_height="75dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="528dp"
        android:src="@drawable/hiok"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/gifImageView2"
        android:layout_width="54dp"
        android:layout_height="81dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="484dp"
        android:src="@drawable/hiok"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/gifImageView3"
        android:layout_width="62dp"
        android:layout_height="60dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="488dp"
        android:src="@drawable/hiok"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtPathOutcome"
        android:layout_width="320dp"
        android:layout_height="57dp"
        android:layout_marginTop="176dp"
        android:fontFamily="@font/alphakind"
        android:text="TextView"
        android:textAlignment="center"
        android:textColor="#403D3D"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.67"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnPathProceed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="92dp"
        android:fontFamily="@font/alphakind"
        android:text="Button"
        app:backgroundTint="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/txtPathOutcome"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/txtPathOutcome"
        app:layout_constraintTop_toBottomOf="@+id/txtPathOutcome" />

</androidx.constraintlayout.widget.ConstraintLayout>